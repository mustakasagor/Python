A = {1, 2, 3, 4, 5}
B = {4, 5, 6, 7, 8}

print("Set A:", A)
print("Set B:", B)

print("\nUnion of A and B:", A | B)

print("Intersection of A and B:", A & B)

print("Difference of A and B (A - B):", A - B)
print("Difference of B and A (B - A):", B - A)

print("Symmetric Difference of A and B:", A ^ B)  # or A.symmetric_difference(B))
