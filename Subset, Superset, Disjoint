
A = {1, 2, 3, 4}
B = {3, 4, 5, 6}

print("Set A:", A)
print("Set B:", B)

if A.issubset(B):
    print("A is a subset of B")
else:
    print("A is NOT a subset of B")

if A.issuperset(B):
    print("A is a superset of B")
else:
    print("A is NOT a superset of B")

if A.isdisjoint(B):
    print("A and B are disjoint sets")
else:
    print("A and B are NOT disjoint sets")
